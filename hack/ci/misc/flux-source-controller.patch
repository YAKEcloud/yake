spec:
  template:
    spec:
      containers:
      - name: manager
        volumeMounts:
        - mountPath: /etc/ssl/certs/le-staging.pem
          name: le-staging
          subPath: le-staging.pem
        - mountPath: /etc/ssl/certs/local-ca.pem
          name: local-ca
          subPath: local-ca.pem
      volumes:
      - name: le-staging
        configMap:
          name: le-staging
      - name: local-ca
        configMap:
          name: local-ca
